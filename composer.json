{
    "name": "mkato/library-calendar",
    "require": {
        "google/apiclient": "^2.2",
        "nesbot/carbon": "^2.41",
        "php": "^7.1",
        "laravel/framework": "^5.8"
    },
    "description": "An API extension for getting free time from users calendar",
    "authors": [
        {
            "name": "masafumi kato",
            "email": "xslash1105@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mkato\\Library\\Calendar\\" : "src/Calendar"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkato\\Library\\Tests\\" : "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mkato\\Library\\Calendar\\Laravel\\ServiceProvider"
            ]
        }
    }
}
